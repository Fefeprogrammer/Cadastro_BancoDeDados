
@{
    ViewBag.Title = "Formulario";
}

<h2>Formulario</h2>

<form>
    <label for="tnome">Nome:</label>
    <input type="text" id="tnome" name="cnome" />
    <br>
    <label for="tidade">Idade:</label>
    <input type="number" id="tidade" name="cIdade" />
