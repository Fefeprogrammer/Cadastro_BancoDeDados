    <label for="temail">E-mail:</label>
    <input type="email" id="temail" name="cemail" />
    <br>
    <label for="tendereco">Endereço:</label>
    <input type="text" id="tendereco" name="cendereco">
    <br>
    <input type="submit" value="Cadastrar">
</form>